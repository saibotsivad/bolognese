{
  "name": "bolognese",
  "version": "0.0.0",
  "description": "A spicy Ragu recipe to turn Markdown into HTML.",
  "type": "module",
  "module": "dist/index.js",
  "unpkg": "dist/index.min.js",
  "main": "dist/index.cjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "npm run test:lint && npm run test:unit && npm run test:demo",
    "test:lint": "eslint 'src/*.js'",
    "test:demo": "ragu -c example/ragu.config.js",
    "build": "bundt src/index.js",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saibotsivad/bolognese.git"
  },
  "keywords": [
    "ragu",
    "noddity",
    "11ty",
    "jekyll"
  ],
  "author": {
    "name": "Tobias Davis",
    "email": "tobias@davistobias.com",
    "url": "https://davistobias.com"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/saibotsivad/bolognese/issues"
  },
  "homepage": "https://github.com/saibotsivad/bolognese",
  "devDependencies": {
    "@saibotsivad/eslint-config-saibotsivad": "*",
    "ragu": "^1.0.1"
  },
  "dependencies": {
    "@saibotsivad/blockdown": "^3.0.0",
    "dlv": "^1.1.3",
    "js-yaml": "^4.1.0"
  }
}
